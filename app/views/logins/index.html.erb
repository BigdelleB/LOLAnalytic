<div class="page-header">
  <div class="center-block">
  <h1>Welcome</h1>
</div>
</div>


<table>
  <thead>
    <tr>

     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logins.each do |login| %>
      <tr>
        <td><%= login.Username %></td>
        <td><%= login.Region %></td>
        <td><%= link_to 'Show', login %></td>
        <td><%= link_to 'Edit', edit_login_path(login) %></td>
        <td><%= link_to 'Destroy', login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <p>
          
        </p>
      </tr>
    <% end %>
  </tbody>
</table>


<br>
<div class="login_button">
  <%= link_to 'New Login', new_login_path, class:"btn btn-success" %>
</div>

