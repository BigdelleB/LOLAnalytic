<body id="background">
<div class="page-header">
  <div class="center-block">
  <h1>Welcome to LOLAnalytic</h1>
  <p>The most personal League of Legends tool yet</p>
</div>
</div>


<table>
  <thead>
    <tr>

     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logins.each do |login| %>
      <tr>
        <div class= "username">
          <td><%= login.Username,  :id => "username" %></td>
        </div>
        <td><%= link_to 'Edit', edit_login_path(login) %></td>
        <td><%= link_to 'Destroy', login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Stats', data_index_path(params: login.attributes) %></td>
        <p>
          
        </p>
      </tr>
    <% end %>
  </tbody>
</table>


<br>
<div class="login_button">
  <%= link_to 'Login', new_login_path, class:"btn btn-success" %>
</div>
</body>
