<body id="background">
<div class="page-header">
  <div class="center-block">
  <h1>Welcome to LOLAnalytic</h1>
  <p>The most personal League of Legends tool yet</p>
</div>
</div>


<table>
  <thead>
    <tr>

     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logins.each do |login| %>
      <tr>
        
          <td div class='username' id='login<%=login.id%>'>
            <%= login.Username%>
          </td>

          <td><%= link_to 'Edit', edit_login_path(login), :id => 'edit_option' %></td>
          <td><%= link_to 'New', login, method: :delete, data: { confirm: 'Are you sure?' }, :id => 'destroy_option' %></td>
          <td><%= link_to 'Stats', data_index_path(params: login.attributes), :id => 'stats_option' %></td>

        <p>
          
        </p>
      </tr>
    <% end %>
  </tbody>
</table>


<br>
<div class="login_button">
  <%= link_to 'Login', new_login_path, class:"btn btn-success" %>
</div>
</body>
